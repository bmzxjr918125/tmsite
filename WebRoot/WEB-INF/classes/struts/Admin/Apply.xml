<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package namespace="/admin" name="apply" extends="tmsiteadmin">
	
        <action name="apply*" class="adminOperatorApplyAction" method="{1}">
        	
            <result name="Show">/WEB-INF/page/apply/applyList.jsp</result>
            
            <result name="answer" type="json">
				<param name="root">answer</param>
			</result>
			
			<result name="AjaxShow" type="json">
				<param name="root">dtJson</param>
			</result>
            
        </action>
        
    </package>
</struts>
